<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cencol.kevinma_comp304lab4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cencol.kevinma_comp304Lab4_Ex1.LoginActivity"
            android:label="@string/login_label"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name="com.cencol.kevinma_comp304Lab4_Ex1.HomeActivity"
            android:label="@string/home_label"
            android:parentActivityName="com.cencol.kevinma_comp304Lab4_Ex1.LoginActivity" />
        <activity android:name="com.cencol.kevinma_comp304Lab4_Ex1.BaseNavigationActivity"></activity>
    </application>

</manifest>